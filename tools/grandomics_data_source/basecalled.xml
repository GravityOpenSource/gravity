<?xml version="1.0"?>
<tool id="load_from_basecalled" name="加载数据" version="0.1.0">
    <description>从Basecalled目录加载指定cell的fastq_pass数据</description>
    <command>
        python '$__tool_directory__/basecalled.py' -n $cell -r $pass_required -n $pass_required -o outputs
    </command>
    <inputs>
        <param name="cell" type="text" label="Cell名称" value="20190219-TEST-P1"/>
        <param name="pass_required" type="boolean" truevalue="true" falsevlaue="false" value="true"
               label="是否仅加载PASS FASTQs(文件名带有_pass_)"/>
        <param name="number" type="integer" label="Cell名称" value="20190219-TEST-P1"/>
    </inputs>
    <outputs>
        <collection name="list_output" type="list" label="BASECALLED FASTQs">
            <discover_datasets pattern="__name_and_ext__" directory="outputs"/>
        </collection>
    </outputs>
    <!--    <help><![CDATA[
            TODO: Fill in help.
        ]]></help>-->
</tool>