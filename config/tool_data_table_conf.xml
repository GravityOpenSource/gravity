<!-- Paths are relative to the value of `tool_data_path` in galaxy.ini -->
<tables>
    <!-- Locations of all fasta files under genome directory -->
    <table name="all_fasta" comment_char="#" allow_duplicate_entries="False">
        <columns>value, dbkey, name, path</columns>
        <file path="tool-data/all_fasta.loc" />
    </table>
    <!-- Locations of primer fasta files under genome directory -->
    <table name="primer_fasta" comment_char="#" allow_duplicate_entries="False">
        <columns>value, dbkey, name, path</columns>
        <file path="tool-data/primer_fasta.loc" />
    </table>
    <!-- Locations of snpEff annotation database -->
    <table name="snpeffv_genomedb" comment_char="#" allow_duplicate_entries="False">
        <columns>key, version, value, name, path, config</columns>
        <file path="tool-data/snpeffv_genomedb.loc" />
    </table>
    <!-- Locations of rampart protocol directory -->
    <table name="rampart_protocol" comment_char="#" allow_duplicate_entries="False">
        <columns>value, dbkey, name, path</columns>
	<file path="tool-data/rampart_protocol.loc" />
    </table>
    <table name="all_gff" comment_char="#" allow_duplicate_entries="False">
        <columns>value, dbkey, name, path</columns>
	<file path="tool-data/all_gff.loc" />
    </table>
</tables>
